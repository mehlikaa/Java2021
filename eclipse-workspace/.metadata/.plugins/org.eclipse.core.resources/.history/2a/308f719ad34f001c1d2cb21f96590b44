package CG02_Constructors;

public class Car {

	int yil=8;
	String model;
	String renk;
	int motor;
	int beygir;
	boolean satilik;

	
	
	Car(int yil1, String model) {
		
		this.model=model;
		this.yil=yil1;	
	}
	
	
	Car(){
		
	}
	
	public Car(int yil, String model, String renk, int beygirc) {
		beygir=beygirc;

	}

}
