package day27_constructor_constructorCall;

public class Car1 {

	
	int yil = 2000;
	String renk;
	String model;
	int km;
	boolean satilikMi;
	

	public void hiz(int hiz) {
		System.out.println("araba saatte " + hiz + " km yapar");
	}

	public void yakit(String yakit) {
		System.out.println("Araba yakit olarak " + yakit + " kullanir");
	}
}
