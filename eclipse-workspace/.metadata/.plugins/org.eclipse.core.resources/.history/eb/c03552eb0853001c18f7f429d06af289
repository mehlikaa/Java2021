package day30_immutableClasses;

public class C04_Homeworksorusu {

	public static void main(String[] args) {
		
		String a="";
		System.out.println(a+=2);
		System.out.println(a+='c');
		System.out.println(a+=false);
		
		if (a=="2cfalse") {
			System.out.println("==");
		}
		
		if (a.equals("2cfalse")) {
			System.out.println("equals");
		}
		
	}

}
